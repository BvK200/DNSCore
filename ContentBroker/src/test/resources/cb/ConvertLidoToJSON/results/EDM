      <edm:hasView rdf:resource="http://duesseldorf.emuseum.net/media/previews/02_gespenster_1905.jpg"/>
      <edm:hasView rdf:resource="http://duesseldorf.emuseum.net/media/full/02_gespenster_1905.jpg"/>
