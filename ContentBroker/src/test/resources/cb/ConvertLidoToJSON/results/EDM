      <edm:hasView rdf:resource="http://duesseldorf.emuseum.net/media/full/HM.2014%20Feuerskulptur_SS16.jpg"/>
      <edm:hasView rdf:resource="http://duesseldorf.emuseum.net/media/full/HM.2014%20Feuerskulptur_SS16.jpg"/>
