					<lido:workID lido:type="Inventarnummer">HHI.Rkult.vormaerz16</lido:workID>
					<lido:repositoryLocation>
							<lido:namePlaceSet>
		<lido:appellationValue><![CDATA[Düsseldorf]]></lido:appellationValue>
	</lido:namePlaceSet>
