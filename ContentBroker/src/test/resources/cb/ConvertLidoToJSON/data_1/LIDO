	<lido:lidoRecID lido:type="local" lido:source="Digitales Kunst- und Kulturarchiv Düsseldorf">DE-MUS-042628:DE-MUS-042524:133656</lido:lidoRecID>
	<lido:objectPublishedID lido:type="local">http://www.duesseldorf.de/dkult/DE-MUS-042524/133656</lido:objectPublishedID>
	<lido:descriptiveMetadata xml:lang="de">
		<lido:objectClassificationWrap>
			<lido:objectWorkTypeWrap>
				<lido:objectWorkType lido:type="Objektbezeichnung" lido:sortorder="1">
					<lido:term lido:label="Objektbezeichnung">Reiterstandbild</lido:term>
				</lido:objectWorkType>
			</lido:objectWorkTypeWrap>
			<lido:classificationWrap>
				<lido:classification lido:type="Sachgruppe">
